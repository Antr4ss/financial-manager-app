{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/app.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "frontend/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "frontend/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "frontend/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}